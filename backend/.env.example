# PropIQ Backend Environment Variables
# Copy this file to .env and fill in your values

# Sentry Configuration (Get from https://sentry.io)
SENTRY_DSN=https://YOUR_BACKEND_DSN_HERE@o123456.ingest.sentry.io/7890456
SENTRY_ENVIRONMENT=development
SENTRY_RELEASE=propiq-backend@1.0.0

# Existing environment variables...
AZURE_OPENAI_ENDPOINT=your_endpoint
AZURE_OPENAI_KEY=your_key
AZURE_OPENAI_API_VERSION=2024-02-15-preview
MONGODB_URI=your_mongodb_uri
STRIPE_SECRET_KEY=your_stripe_key
STRIPE_PRICE_ID=your_price_id
STRIPE_WEBHOOK_SECRET=your_webhook_secret
WANDB_API_KEY=your_wandb_key
WANDB_MODE=online
SENDGRID_API_KEY=your_sendgrid_key

# For production (Azure App Settings):
# SENTRY_ENVIRONMENT=production
