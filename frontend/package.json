    "test:env": "playwright test tests/env-validation.spec.ts",
    "test:env:production": "PLAYWRIGHT_BASE_URL=https://luntra.one VITE_API_BASE=https://api.luntra.one playwright test tests/env-validation.spec.ts",
    "test:integration": "playwright test tests/user-signup-integration.spec.ts",
    "test:integration:local": "VITE_API_BASE=http://localhost:8000 playwright test tests/user-signup-integration.spec.ts",
    "test:integration:production": "VITE_API_BASE=https://api.luntra.one playwright test tests/user-signup-integration.spec.ts",
    "mongodb:verify": "node scripts/verify-mongodb-signup.cjs",
    "deploy:verify": "node scripts/verify-deployment.cjs",
    "deploy:verify:local": "node scripts/verify-deployment.cjs http://localhost:8000",
    "deploy:verify:production": "node scripts/verify-deployment.cjs https://api.luntra.one",
    "test:backend-deploy": "playwright test tests/backend-deployment.spec.ts",
    "test:backend-deploy:local": "VITE_API_BASE=http://localhost:8000 playwright test tests/backend-deployment.spec.ts",
    "test:backend-deploy:production": "VITE_API_BASE=https://api.luntra.one playwright test tests/backend-deployment.spec.ts",
    "deploy:frontend": "bash scripts/deploy-cloudflare.sh",
    "deploy:frontend:purge": "bash scripts/deploy-cloudflare.sh --purge-cache",
    "test:convex": "playwright test tests/convex-integration.spec.ts",
    "test:convex:headed": "playwright test tests/convex-integration.spec.ts --headed",
    "test:chaos": "playwright test tests/chaos-engineering.spec.ts",
    "test:chaos:headed": "playwright test tests/chaos-engineering.spec.ts --headed",
    "test:all": "npm run test:health && npm run test:convex && npm run test:integration",
    "test:resilience": "npm run test:chaos && npm run test:health",
    "test:production:backend": "PLAYWRIGHT_BASE_URL=https://propiq.luntra.one playwright test tests/production-backend-integration.spec.ts --reporter=list",
    "test:production:backend:headed": "PLAYWRIGHT_BASE_URL=https://propiq.luntra.one playwright test tests/production-backend-integration.spec.ts --headed",
    "test:production:full": "bash scripts/verify-production-integration.sh",
    "test:production:signup": "PLAYWRIGHT_BASE_URL=https://propiq.luntra.one playwright test --grep 'user signup' tests/production-backend-integration.spec.ts",
    "test:production:analysis": "PLAYWRIGHT_BASE_URL=https://propiq.luntra.one playwright test --grep 'Property Analysis' tests/production-backend-integration.spec.ts",
    "verify:production": "bash scripts/verify-production-integration.sh",
    "test:password-reset": "playwright test tests/password-reset.spec.ts",
    "test:password-reset:ui": "playwright test tests/password-reset.spec.ts --ui",
    "test:password-reset:headed": "playwright test tests/password-reset.spec.ts --headed",
    "test:password-reset:debug": "playwright test tests/password-reset.spec.ts --debug",
    "test:password-reset:report": "playwright show-report",
    "test:password-reset:integration": "ENABLE_INTEGRATION_TESTS=true playwright test tests/password-reset.spec.ts",
    "test:password-reset:gmail": "playwright test tests/password-reset-with-gmail.spec.ts",
    "test:password-reset:gmail:headed": "playwright test tests/password-reset-with-gmail.spec.ts --headed",
    "test:password-reset:gmail:debug": "PWDEBUG=1 playwright test tests/password-reset-with-gmail.spec.ts",
    "test:account-settings": "playwright test tests/account-settings.spec.ts",
    "test:account-settings:headed": "playwright test tests/account-settings.spec.ts --headed",
    "test:account-settings:ui": "playwright test tests/account-settings.spec.ts --ui",
    "test:change-password": "playwright test tests/change-password.spec.ts",
    "test:change-password:headed": "playwright test tests/change-password.spec.ts --headed",
    "test:change-password:ui": "playwright test tests/change-password.spec.ts --ui",
    "test:subscription": "playwright test tests/subscription-management.spec.ts",
    "test:subscription:headed": "playwright test tests/subscription-management.spec.ts --headed",
    "test:subscription:ui": "playwright test tests/subscription-management.spec.ts --ui",
    "test:preferences": "playwright test tests/preferences.spec.ts",
    "test:preferences:headed": "playwright test tests/preferences.spec.ts --headed",
    "test:preferences:ui": "playwright test tests/preferences.spec.ts --ui",
    "test:account-maintenance": "playwright test tests/account-settings.spec.ts tests/change-password.spec.ts tests/subscription-management.spec.ts tests/preferences.spec.ts --reporter=list",
    "test:account-maintenance:headed": "playwright test tests/account-settings.spec.ts tests/change-password.spec.ts tests/subscription-management.spec.ts tests/preferences.spec.ts --headed",
    "analytics": "node scripts/analytics-dashboard.js",
    "analytics:export": "node scripts/analytics-dashboard.js --export"
  },
