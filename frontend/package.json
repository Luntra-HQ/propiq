{
  "name": "outreach-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build",
    "build:strict": "tsc && vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:pre-deploy": "playwright test tests/pre-deployment.spec.ts",
    "test:health": "playwright test tests/deployment-health.spec.ts",
    "test:production": "PLAYWRIGHT_BASE_URL=https://luntra.one playwright test tests/pre-deployment.spec.ts",
    "monitor": "bash scripts/continuous-monitor.sh monitor",
    "monitor:once": "bash scripts/continuous-monitor.sh once",
    "slack:test": "node scripts/test-webhook.cjs",
    "comet:check": "node scripts/comet-integration.cjs",
    "simulate:degrade": "node scripts/simulate-degradation.cjs",
    "remediate": "bash scripts/auto-remediation.sh",
    "test:visual": "PLAYWRIGHT_BASE_URL=https://luntra.one playwright test tests/visual-regression.spec.ts",
    "snapshots:update": "PLAYWRIGHT_BASE_URL=https://luntra.one playwright test tests/visual-regression.spec.ts --update-snapshots",
    "snapshots:upload": "node scripts/upload-snapshots-to-comet.cjs",
    "logs:archive": "node scripts/archive-logs-to-comet.cjs",
